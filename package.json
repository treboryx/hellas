{
  "name": "hellas",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@7.17.1",
  "description": "Greek Tax ID, Greek Social Security Number, and Greek Postal Code validators",
  "author": "treboryx",
  "license": "MIT",
  "funding": "https://github.com/sponsors/treboryx",
  "homepage": "https://github.com/treboryx/hellas#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/treboryx/hellas.git"
  },
  "bugs": "https://github.com/treboryx/hellas/issues",
  "keywords": [
    "amka",
    "afm",
    "vat",
    "greek",
    "validation",
    "passport",
    "id"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.18.8",
    "@types/node": "^18.11.18",
    "bumpp": "^8.2.1",
    "eslint": "^8.31.0",
    "esno": "^0.16.3",
    "pnpm": "^7.21.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "vite": "^4.0.3",
    "vitest": "^0.27.0"
  }
}
